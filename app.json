{
  "expo": {
    "name": "react-native-webview-quilljs",
    "description": "Development and test application for a react native webview component incorporating Quill JS",
    "slug": "react-native-webview-quilljs",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.reggie3.reactnativewebviewquilljs",
      "supportsTablet": true
    },
    "packagerOpts": {
      "assetExts": [
        "html"
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.reggie3.reactnativewebviewquilljs"
    }
  }
}
